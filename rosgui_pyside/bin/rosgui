#!/usr/bin/env python

import os
import sys

try:
    import rospkg
    r = rospkg.RosPack()
    p = r.get_path('rosgui')
    sys.path.append(os.path.join(p, 'src'))
except ImportError:
    import roslib
    roslib.load_manifest('rosgui')

sys.argv.append('--qt-binding=pyside')

from rosgui import rosgui_main
rosgui_main.rosgui_main()
