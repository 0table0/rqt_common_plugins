#!/usr/bin/env python

import os
import sys

try:
    import rospkg
    r = rospkg.RosPack()
    p = r.get_path('qt_gui')
    sys.path.append(os.path.join(p, 'src'))
except ImportError:
    import roslib
    roslib.load_manifest('qt_gui')

from qt_gui.Main import Main
main = Main()
# TODO add dummy plugin provider 
sys.exit(main.main())
