<package>
  <description brief="Foundation for plugins using rospy">

     rosgui_rospy enables rosgui plugins to use the Python client library for ROS.

  </description>
  <author>Dirk Thomas</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/rosgui_rospy</url>

  <depend package="rosgui"/>
  <depend package="rospy"/>

  <export>
    <rosgui plugin="${prefix}/rospy_plugin.xml"/>
  </export>

</package>


