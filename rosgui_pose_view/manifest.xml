<package>
  <description brief="Plugin for visualizing 3D poses">

     rosgui_pose_view provides a rosgui plugin for visualizing 3D poses.

  </description>
  <author>Dorian Scholz</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/rosgui_pose_view</url>

  <depend package="rosgui"/>
  <depend package="rosgui_rospy"/>
  <depend package="rostopic"/>
  <depend package="geometry_msgs"/>
  <depend package="tf"/>

  <export>
    <rosgui plugin="${prefix}/pose_view_plugin.xml"/>
  </export>

</package>

